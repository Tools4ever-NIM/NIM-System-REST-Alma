{
    "schema": {
        "options": {},
        "crud_objects": {
            "schools": {
                "operations": {
                    "schoolsList": {
                        "method": "get",
                        "call": {
                            "path": "/v2/{tenant_id}/district/schools"
                        },
                        "processing_options": {
                            "output_field": "response.schools"
                        },
                        "query_parameters": {
                        }
                    }
                },
                "parameters": {},
                "key": "id",
                "resources": {
                    "id": "string*",
                    "name": "string*",
                    "url": "string*",
                    "stateId": "string*",
                    "districtId": "string*",
                    "settings": {
                        "credits": {
                            "trackCredits": "_:boolean*",
                            "defaultCredits": "_:string*"
                        },
                        "transcripts": {
                            "enabled": "_:boolean*",
                            "courseAllocation": "_:boolean*"
                        }
                    },
                    "created": "string*",
                    "modified": "string*"
                }
            },
            "students": {
                "operations": {
                    "studentList": {
                        "method": "get",
                        "call": {
                            "mode": "iteration",
                            "table": "schools",
                            "iterator": "id",
                            "path": "/v2/{iterator}/students",
                            "base": {
                                "school_id": "{iterator}"
                            }
                        },
                        "processing_options": {
                            "output_field": "response"
                        },
                        "query_parameters": {
                        },
                        "maxSessionCount": 20
                    },
                    "studentUpdateEmail": {
                        "method": "put",
                        "call": {
                            "mode": "normal",
                            "path": "/v2/{school_id}/students/{id}/contact/email"
                        },
                        "resource_allowance_default": "prohibited",
                        "resource_mandatory": [
                            "id",
                            "school_id",
                            "email"
                        ],
                        "warning_only": [],
                        "body":{
                            "_nim_merge_attributes_": true,
                            "email": [{
                                "emailAddress": "{email}"
                            }]
                        }
                    }
                },
                "parameters": {},
                "key": "id",
                "resources": {
                    "school_id": "string*",
                    "id": "string*",
                    "username": "string*",
                    "firstName": "string*",
                    "middleName": "string*",
                    "lastName": "string*",
                    "preferredName": "string*",
                    "schoolId": "string*",
                    "districtId": "string*",
                    "stateId": "string*",
                    "gender": "string*",
                    "ethnicity": "string*",
                    "race": ["string*"],
                    "dob": "string*",
                    "languages": ["string*"],
                    "graduationYear": "string*",
                    "status": "string*",
                    "externalIds": ["string*"],
                    "created": "string*",
                    "modified": "string*",
                    "email": "string"
                }
            },
            "students_contact_email": {
                "operations": {
                    "students_contact_emailList": {
                        "method": "get",
                        "call": {
                            "mode": "iteration",
                            "table": "students",
                            "iterator": "id",
                            "path": "/v2/{tenant_id}/students/{iterator}/contact/email"
                        },
                        "processing_options": {
                            "output_field": "response"
                        },
                        "query_parameters": {
                        },
                        "maxSessionCount": 20
                    }
                },
                "parameters": {},
                "key": "id",
                "resources": {
                    "school_id": "string*",
                    "id": "string*",
                    "email": [
                        {
                            "emailAddress": "_:string*"
                        }
                    ],
                    "created": "string*",
                    "modified": "string*"
                }
            }
        }
    },
    "rest": {
        "config": {
            "baseUrl": "https://{tenant_id}.api.getalma.com",
            "get": {
                "query_parameters": {
                }
            },
            "authentication": "digest",
            "call_handling": "generic",
            "test_connection": {
                "url": "/district/schools"
            },
            "accept": "application/json",
            "retry": [
                {
                    "type": "statusCode",
                    "statusCode": 429,
                    "retry": {
                        "type": "simple",
                        "maxCount": 12,
                        "waitTime": 5
                    }
                },
                {
                    "type": "connectionError"
                }
            ]
        }
    },
    "connection": {
        "items": [
            {
                "name": "tenant_id",
                "type": "textbox",
                "label": "Tenant ID:",
                "value": ""
            }
        ]
        
    }
}
